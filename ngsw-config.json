{
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/index.html",
            "/*.css",
            "/*.js"
          ]
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**",
            "/*.png",
            "/*.ico",
            "/*.svg",
            "/*.jpg",
            "/*.jpeg"
          ]
        }
      }
    ],
    "dataGroups": [
      {
        "name": "api-calls",
        "urls": [
          "https://pokeapi.co/api/v2/**"
        ],
        "cacheConfig": {
          "maxSize": 100,
          "maxAge": "1d",
          "strategy": "freshness"
        }
      }
    ]
  }
  